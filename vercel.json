{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/public/favicon.ico", "dest": "/favicon.ico" },
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/"
    }
  ],
  "REWRITES": [{ "SOURCE": "/(.*)", "DESTINATION": "/" }]
}
